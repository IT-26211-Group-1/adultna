# Security Keys
ARCJET_KEY=YOUR_SECRET_KEY
NEXT_PUBLIC_APP_SECRET=your-secure-secret-key-change-this

# API Configuration (Direct Lambda endpoints - no Next.js API routes)
NEXT_PUBLIC_AUTH_SERVICE_URL=https://your-api-gateway-id.execute-api.us-east-1.amazonaws.com/dev

# Legacy (can be removed when fully migrated)
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000/
NEXTAUTH_URL=http://localhost:3000

# Recaptcha (if needed for additional security)
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=YOUR_SECRET_KEY
RECAPTCHA_SECRET_KEY=YOUR_SECRET_KEY

# Google OAuth (for direct Lambda integration)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.googleusercontent.com
GOOGLE_CLIENT_ID=YOUR_SECRET_KEY
GOOGLE_CLIENT_SECRET=YOUR_SECRET_KEY
GOOGLE_REDIRECT_URI=YOUR_SECRET_KEY

