{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false
  },
  "env": {
    "targets": {
      "chrome": "120",
      "firefox": "120",
      "safari": "16.4",
      "edge": "120"
    },
    "mode": "usage",
    "coreJs": "3"
  },
  "minify": true
}
